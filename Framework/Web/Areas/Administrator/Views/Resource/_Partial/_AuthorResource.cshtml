@using Transverse
<div class="ibox float-e-margins">
    <div class="ibox-title">
        <h5>Author Resource List</h5>
        <div class="ibox-tools">
            <a class="collapse-link">
                <i class="fa fa-chevron-up"></i>
            </a>
        </div>
    </div>
    <div class="ibox-content">
        <div class="table-responsive data-table-author-box">
            <table class="table table-striped table-bordered table-hover data-table-author" style="width: 100%">
                <thead>
                <tr>
                    <th class="col-lg-4">Author</th>
                    <th class="col-lg-4">Url</th>
                    <th class="col-lg-4">Tag</th>
                </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>
